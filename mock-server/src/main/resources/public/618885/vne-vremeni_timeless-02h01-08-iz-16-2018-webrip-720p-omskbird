var bookmarked = 0;
function add_bookmark(me, id)
{
	if (bookmarked == 0)
	{
		$.ajax({
	  type: "POST",
		url: "/bookmarks.php",
	  data: "access_method=ajax&action=add_bookmark&torrent_id=" + id,
	  success: function(res){
	  		if (res == 0)
	  		{
	  			me.html('Зайдите на трекер!');
	  			me.css({"color":"red"});
	  		}
	  		else if (res == 3)
