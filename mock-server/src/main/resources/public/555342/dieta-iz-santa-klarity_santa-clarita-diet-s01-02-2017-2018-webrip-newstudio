		if (typeof torrent_moderate == typeof Function && $.cookie('class') >= 6) {
			$("#cem").append('<br /><span onClick="torrent_moderate(555342);">Промодерировать раздачу</span> | <span onclick="confirm_delete(555342);">Удалить</span>');
		}
	}
});
</script>


