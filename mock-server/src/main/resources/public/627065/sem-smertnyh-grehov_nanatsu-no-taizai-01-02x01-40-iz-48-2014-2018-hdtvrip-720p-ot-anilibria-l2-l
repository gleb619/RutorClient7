	if ($.cookie('userid') > 0) {
		$("#cem").append(' | <a href="/edit.php?edit=627065">Редактировать</a>');
		if (typeof torrent_moderate == typeof Function && $.cookie('class') >= 6) {
			$("#cem").append('<br /><span onClick="torrent_moderate(627065);">Промодерировать раздачу</span> | <span onclick="confirm_delete(627065);">Удалить</span>');
		}
	}
});
</script>


